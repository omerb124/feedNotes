{
    "manifest_version": 2,
    "name": "FeedNotes",
    "version": "1.0.0",
    "description": "Notes for your social feeds important posts",
    "author": "OmerB",
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "contextMenus",
        "http://facebook.com/*",
        "https://facebook.com/*",
        "https://www.facebook.com/*",
        "http://www.facebook.com/*"
    ],
    "icons" : {
        "16" : "assets/icons/16.png",
        "32" : "assets/icons/32.png",
        "64" : "assets/icons/64.png",
        "128" : "assets/icons/128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.facebook.com/*"
            ],
            "css": [
                "main.css",
                "assets/css/all.css",
                "assets/css/list-group.css"
                
            ],
            "js": [
                "assets/js/jquery-3.3.1.min.js",
                "note.js",
                "label.js",
                "handleFb.js",
                "utils.js",
                "main.js",
                "assets/js/popper.min.js",
                "assets/js/bootstrap.min.js"
                

            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources" : [
        "assets/img/*",
        "assets/fonts/*"
    ]
}